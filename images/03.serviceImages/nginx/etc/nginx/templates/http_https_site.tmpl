server {
access_log /var/log/nginx/FQDN/https/access.log;
        error_log /var/log/nginx/FQDN/https/error.log;
        error_page 502 /502.html;
        listen 4430;
     resolver RESOLV_IP;
        add_header Strict-Transport-Security "max-age=31536000; includeSubdomains";
        add_header Accept-Ranges bytes;
    server_name FQDN;
    proxy_read_timeout 300s;
    ssl on;
    ssl_session_cache    shared:SSL:1m;
    ssl_session_timeout  10m;
    ssl_certificate /etc/nginx/ssl/certs/CERTNAME.crt;
    ssl_certificate_key /etc/nginx/ssl/keys/CERTNAME.key;
    proxy_ssl_session_reuse off;
    ssl_verify_client off;
    ssl_protocols        SSLv3 TLSv1 TLSv1.1 TLSv1.2;
    ssl_ciphers RC4:HIGH:!aNULL:!MD5;
    ssl_prefer_server_ciphers on;

        location / {
            FOLDER            		
	proxy_pass http://SERVER.engines.internal:PORT/;
    proxy_set_header Upgrade $http_upgrade;
   # proxy_set_header Connection "upgrade";  
    proxy_set_header X-Real-IP $remote_addr;    
    proxy_set_header Host $host;
   proxy_set_header X-Forwarded-For $remote_addr;
   proxy_redirect     off;
        }
        location /502.html{
		}
    }

