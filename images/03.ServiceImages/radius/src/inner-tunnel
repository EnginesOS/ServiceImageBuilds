
server inner-tunnel {


 listen {
       ipaddr = 127.0.0.1
       port = 18120
       type = auth
 }


 authorize {
        chap
        pap
        mschap
        suffix
        update control {
               Proxy-To-Realm := LOCAL
           }
        eap {
                ok = return
        }
        files
        expiration
        logintime
        pap
 }


authenticate {
     
 Auth-Type Kerberos {
   krb5
 }
 Auth-Type PAP {
   pap
 }


        Auth-Type CHAP {
                chap
        }

        Auth-Type MS-CHAP {
                mschap
        }

        eap
 }


 session {
        radutmp
 }

 post-auth {
        Post-Auth-Type REJECT {
                # log failed authentications in SQL, too.
#               sql
                attr_filter.access_reject
        }
}     

 pre-proxy {

}

#

post-proxy {
         #  If you want to have a log of replies from a home server,
    
        eap

        
 }



} # inner-tunnel server block
       