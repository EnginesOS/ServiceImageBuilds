#!/usr/bin/expect

set timeout 10


spawn kdb5_ldap_util  -sf /etc/krb5kdc/ldap/service.keyfile  create -subtrees dc=engines,dc=internal -r ENGINES.INTERNAL -s -H ldap://ldap.engines.internal
# kdb5_util create -r ENGINES.INTERNAL -s

expect "Enter KDC database master key:" { send "$env(pass)\r" }
expect "Re-enter KDC database master key to verify:" { send "$env(pass)\r" }

interact
